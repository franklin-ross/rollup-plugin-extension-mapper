{
  "name": "rollup-plugin-extension-mapper",
  "version": "1.0.1",
  "description": "Changes the file extensions used in imports. Useful in multistage builds where Rollup gets the output of another step.",
  "main": "dist/rollup-plugin-extension-mapper.cjs.js",
  "module": "dist/rollup-plugin-extension-mapper.es.js",
  "scripts": {
    "build:cjs": "babel --presets es2015 src/rollup-plugin-extension-mapper.js --out-file dist/rollup-plugin-extension-mapper.cjs.js",
    "build:es2015": "shx cp src/rollup-plugin-extension-mapper.js dist/rollup-plugin-extension-mapper.es.js",
    "build": "run-p build:es2015 build:cjs",
    "test": "mocha --require babel-core/register",
    "clean:dist": "rimraf dist/*",
    "clean:modules": "rimraf node_modules",
    "clean:files": "rimraf npm-debug.log*",
    "clean": "run-p clean:*"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^3.5.0",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.0.2",
    "shx": "^0.2.2",
    "rimraf":"^2.6.1"
  },
  "author": "Franklin Ross <fhross@gmail.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/franklin-ross/rollup-plugin-extension-mapper.git"
  }
}
